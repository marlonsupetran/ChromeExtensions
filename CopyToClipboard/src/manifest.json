{
  "name": "Copy to Clipboard",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Copies data from web pages to the clipboard.",
  "icons": {
    "19": "images/icon-19.png",
    "128": "images/icon-128.png"
  },
  "permissions": [
    "tabs",
    "*://*.linkedin.com/sales/*",
    "webNavigation"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.linkedin.com/sales/*"
      ],
      "js": [
        "index.bundle.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ]
}